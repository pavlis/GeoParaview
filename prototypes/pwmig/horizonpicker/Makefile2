BIN=horizonpicker
PF=horizonpicker.pf
ldlibs=-lseispp  $(DBLIBS) $(TRLIBS) -lgclgrid -lperf 
ANTELOPEMAKELOCAL = $(ANTELOPE)/local/include/antelopemake.local
include $(ANTELOPEMAKE)  	
include $(ANTELOPEMAKELOCAL)

CXXFLAGS += -I$(MATLAB)/extern/include
#LDLIBS += -L$(MATLAB)/bin/maci64 -R$(MATLAB)/bin/maci64 -rpath $(MATLAB)/bin/maci64 
LDLIBS += -L$(MATLAB)/bin/maci64 -R$(MATLAB)/bin/maci64/ \
  -lmatlabprocessor -leng -lmx -lut -lmat
OBJS=horizonpicker.o 
$(BIN) : $(OBJS)
	$(RM) $@
	$(CXX) $(CXXFLAGS) -o $@ $(OBJS) $(LDFLAGS) $(LDLIBS)
