BIN=test_pwdecon
ldlibs=-lseispp -ltrvltm -lbrttutil $(TRLIBS) $(DBLIBS) $(XMOTIFLIB) \
       -lseisppplot -lseisw -lXm -lXt -lperf -lgclgrid -lpwmig $(X11LIBS)

include $(ANTELOPEMAKE)  	
include $(ANTELOPEMAKE).local

CXXFLAGS += $(XMOTIFINCLUDE)
OBJS=test_pwdecon.o
$(BIN) : $(OBJS)
	$(RM) $@
	$(CXX) $(CCFLAGS) -o $@ $(OBJS) $(LDFLAGS) $(LDLIBS)
